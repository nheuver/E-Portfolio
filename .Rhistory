library(sf)
library(spdep)
library(here)
library(tidyverse)
library(RColorBrewer)
library(mapview)
#Read in woodpecker quadrats (wpc.quads)
wpc.quads <- st_read(here("wpc_quads.gpkg")) %>%
#Add ID column
mutate(ID = 1:nrow(.))
# Read in woodpecker point data (wpc.df)
wpc.df <- st_read(here("wpc_points.gpkg")) %>%
#Convert to sp object
as_Spatial()
#Plot the point data and the quadrats
wpc.df %>%
st_as_sf() %>%
ggplot(aes(color = WP_Counts)) +
geom_sf() +
geom_sf(data = wpc.quads, fill = NA, color = "black") +
scale_color_viridis_c(option = "plasma") +
labs(title = "Woodpecker Counts",
x = "X",
y = "Y") +
theme_bw()
#Generate a simplified plot with low/high wpc counts
wpc.quads %>%
ggplot(aes(fill = WP_Counts >= 10)) +
geom_sf() +
labs(title = "Woodpecker Counts (High vs. Low)",
x = "X",
y = "Y") +
theme_bw()
